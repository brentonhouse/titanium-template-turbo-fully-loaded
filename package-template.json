		"@titanium/polyfill": ">=0.0.6",
		"@titanium/errors": ">=0.0.2",
		"@titanium/screenshot": "latest",
		"@titanium/applesignin": "latest",
		"@titanium/googlesignin": "latest",
		"@titanium/facebook": ">=1.0.10",
		"@titanium/lottie": ">=1.0.18",
		"@titanium/observer": "0.0.2",
		"@titanium/permissions": ">=0.0.6",
		"@titanium/webdialog": ">=1.0.13",
		"@mobile/tinycolor2": ">=1.4.1-2",
