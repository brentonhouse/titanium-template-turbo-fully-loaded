{
	"name": "${name}",
	"version": "1.0.0",
	"description": "${description}",
	"main": "index.js",
	"scripts": {
		"test": "npm install --ignore-scripts && npm run list",
		"lint": "eslint ./app --fix",
		"print": "printenv",
		"prebuild": "ti sdk install",
		"clean": "rm -rfv build && rm -rfv i18n && rm -rfv platform && rm -rfv Resources",
		"build": "npm run build:ios && npm run build:android",
		"build:ios": "ti build -p ios --build-only --log-level trace",
		"build:android": "ti build -p android --build-only --log-level trace",
		"alloy": "turbo2 compile ./app --config platform=ios,version=0,simtype=none,devicefamily=universal,deploytype=development,target=simulator"
	},
	"license": "${license}",
	"dependencies": {
		"@geek/cache": "^1.0.1",
		"@geek/logger": "^1.0.8",
		"@mobile/tinycolor2": "^1.4.1-2",
		"@titanium/authentication": "^1.0.0",
		"@titanium/authentication-oauth": "^1.0.5",
		"@titanium/bluebird": "^3.7.2-1",
		"@titanium/calendar-picker": "0.0.11",
		"@titanium/deeply": "^1.0.4",
		"@titanium/essentials": "^1.0.15",
		"@titanium/identity": ">=1.1.1",
		"@titanium/jsonc": "0.0.10",
		"@titanium/jwt": "^1.0.0",
		"@titanium/lottie": "^1.0.24",
		"@titanium/observer": "0.0.3",
		"@titanium/permissions": ">=0.0.6",
		"@titanium/please": "^1.0.10",
		"@titanium/polyfill": "0.0.15",
		"@titanium/screenshot": "latest",
		"@titanium/updater": "^1.0.1",
		"@titanium/webdialog": "^2.1.3",
		"ansi-colors": "^4.1.1",
		"@titanium/googlesignin": "latest",
		"@titanium/facebook": ">=1.0.10"
	},
	"devDependencies": {
		"@titanium/module-copier": "0.0.28",
		"@titanium/plugin-turbo": ">=1.0.6",
		"@titanium/turbo": "^2.0.2",
		"@titanium/eslint-config": "0.0.18",
		"bluebird": "^3.7.2",
		"eslint": "^7.11.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^30.6.4",
		"eslint-plugin-promise": "^4.2.1",
		"fs-extra": "^9.0.1",
		"lodash": "^4.17.20",
		"titanium": ">=5.2.5",
		"handlebars": "^4.7.6",	
		"prettier": "^2.1.2",
		"gm": "^1.23.1"
	},
	"author": {
		"name": "${author_name}",
		"email": "${author_email}",
		"url": "${author_url}"
	},
	"repository": {
		"type": "${repo_type}",
		"url": "${repo_url}"
	},
	"bugs": {
		"url": "${bugs_url}",
		"email": "${bugs_email}"
	},
	"keywords": ["titanium", "mobile", "alloy", "turbo", "appcelerator", "axway", "amplify"]
}
